    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="aa-banner-area">
                        <a href="#"><img src="~/MarkUps-dailyShop/dailyShop/img/fashion-banner-2.jpg" alt="fashion banner img"></a>
