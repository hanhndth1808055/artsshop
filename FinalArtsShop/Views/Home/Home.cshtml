@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Home/Layout/_Layout.cshtml";
}
@{
    var data = ViewBag.Message;
}

<!-- Start slider -->
<section id="aa-slider">
    <div class="aa-slider-area">
        <div id="sequence" class="seq">
            <div class="seq-screen">
                <ul class="seq-canvas">
                    <!-- single slide item -->
                    <li>
                        <div class="seq-data">
                            <img data-seq src="~/MarkUps-dailyShop/dailyShop/img/slider/slideshowbanner-1920x700.jpg" alt="Men slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-data">
                            <img data-seq src="~/MarkUps-dailyShop/dailyShop/img/slider/slideshowbanner2-1920x700.jpg" alt="Wristwatch slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-data">
                            <img data-seq src="~/MarkUps-dailyShop/dailyShop/img/slider/slideshowbanner3-1920x700.jpg" alt="Women Jeans slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-data">
                            <img data-seq src="~/MarkUps-dailyShop/dailyShop/img/slider/slideshowbanner4-1920x700.jpg" alt="Shoes slide img" />
                        </div>
                    </li>
                </ul>
            </div>
            <!-- slider navigation btn -->
            <fieldset class="seq-nav" aria-controls="sequence" aria-label="Slider buttons">
                <a class="seq-prev" aria-label="Previous"><span class="fa fa-angle-left"></span></a>
                <a class="seq-next" aria-label="Next"><span class="fa fa-angle-right"></span></a>
            </fieldset>
        </div>
    </div>
</section>
<!-- / slider -->
<!-- Products section -->
<section id="aa-product">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="aa-product-area">
                        <div class="aa-product-inner">
                            <!-- start prduct navigation -->
                            <ul class="nav nav-tabs aa-products-tab">
                                @{
                                    for (int i = 0; i < data.CategoriesProduct.Count; i++)
                                    {
                                        if (i == 0)
                                        {
                                            <li class="active"><a href="#@data.CategoriesProduct[i].Id" data-toggle="tab">@data.CategoriesProduct[i].Name</a></li>
                                        }
                                        else
                                        {
                                            <li><a href="#@data.CategoriesProduct[i].Id" data-toggle="tab">@data.CategoriesProduct[i].Name</a></li>
                                        }
                                    }
                                }
                            </ul>
                            <!-- Tab panes -->
                            <div class="tab-content">
                                @{
                                    for (int i = 0; i < @data.CategoriesProduct.Count; i++)
                                    {
                                        <!-- start electronic product category -->
                                        <div class="tab-pane fade @{ if (i==0) { @Html.Raw("in active") } }" id="@data.CategoriesProduct[i].Id">
                                            <ul class="aa-product-catg">
                                                @{
                                                    for (int j = 0; j < data.Products.Count; j++)
                                                    {
                                                        if (data.Products[j].Category.Parent == data.CategoriesProduct[i].Id)
                                                        {
                                                            <!-- start single product item -->
                                                            <li>
                                                                <figure>
                                                                    <a class="aa-product-img" href="/Product/Index?id=@data.Products[j].Id"><img src="@("https://res.cloudinary.com/daaycakkk/image/upload/c_limit,h_300,w_250/v1587872991/" + @data.Products[j].GetThubnailIds()[0] + ".jpg")" alt=""></a>
                                                                    <a class="aa-add-card-btn addCart" data-id="@data.Products[j].Id"><span class="fa fa-shopping-cart"></span>Add To Cart</a>
                                                                    <figcaption>
                                                                        <h4 class="aa-product-title"><a href="/Product/Index?id=@data.Products[j].Id">@data.Products[j].Name</a></h4>
                                                                        @{
                                                                            if (data.Products[j].PromotionPrice > 0)
                                                                            {
                                                                                <span class="aa-product-price">$@data.Products[j].PromotionPrice</span><span class="aa-product-price"><del>$@data.Products[j].UnitPrice</del></span>
                                                                            }
                                                                            else
                                                                            {
                                                                                <span class="aa-product-price">$@data.Products[j].UnitPrice</span>
                                                                            }
                                                                        }
                                                                    </figcaption>
                                                                </figure>
                                                                <div class="aa-product-hvr-content">
                                                                    <a onclick="quickView('@data.Products[j].Id', '@data.Products[j].Name', @data.Products[j].PromotionPrice, @data.Products[j].UnitPrice, '@data.Products[j].Description', '@data.Products[j].Category.Name', '@data.Products[j].Category.Id', '@data.Products[j].Image', '@data.Products[j].GetThubnailIds()[0]', '@data.Products[j].GetThubnailIds()[1]', '@data.Products[j].GetThubnailIds()[2]')" href="#" data-toggle2="tooltip" data-placement="top" title="Quick View" data-toggle="modal" data-target="#quick-view-modal"><span class="fa fa-search"></span></a>
                                                                </div>
                                                                <!-- product badge -->
                                                                @{
                                                                    if (@data.Products[j].Unit <= @data.Products[j].SellIndex)
                                                                    {
                                                                        <span class="aa-badge aa-sold-out">Sold Out!</span>
                                                                    }
                                                                    else if (@data.Products[j].isFeature <= 0 && @data.Products[j].isNew > 0 && @data.Products[j].Unit >= @data.Products[j].SellIndex)
                                                                    {
                                                                        <span class="aa-badge aa-sale">NEW!</span>
                                                                    }
                                                                    else if (@data.Products[j].isFeature > 0 && @data.Products[j].Unit >= @data.Products[j].SellIndex)
                                                                    {
                                                                        <span class="aa-badge aa-hot">HOT!</span>
                                                                    }
                                                                }
                                                            </li>
                                                        }
                                                    }
                                                }
                                            </ul>
                                            @*<a class="aa-browse-btn" href="/Category/Index" style="margin-bottom: 15px">Browse all Product <span class="fa fa-long-arrow-right"></span></a>*@
                                        </div>
                                        <!-- / electronic product category -->
                                    }
                                }
                            </div>
                            <!-- quick view modal -->
                            <div class="modal fade" id="quick-view-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-body">
                                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                            <div class="row">
                                                <!-- Modal view slider -->
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <div class="aa-product-view-slider">
                                                        <div class="simpleLens-gallery-container" id="demo-1">
                                                            <div class="simpleLens-container">
                                                                <div class="simpleLens-big-image-container">
                                                                    <a id="quick-view-image" class="simpleLens-lens-image" data-lens-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/large/polo-shirt-1.png">
                                                                        <img src="~/MarkUps-dailyShop/dailyShop/img/view-slider/medium/polo-shirt-1.png" class="simpleLens-big-image">
                                                                    </a>
                                                                </div>
                                                            </div>
                                                            <div class="simpleLens-thumbnails-container">
                                                                <a id="quick-view-thumbnail-1" href="#" class="simpleLens-thumbnail-wrapper"
                                                                   data-lens-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/large/polo-shirt-1.png"
                                                                   data-big-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/medium/polo-shirt-1.png">
                                                                    <img src="~/MarkUps-dailyShop/dailyShop/img/view-slider/thumbnail/polo-shirt-1.png" width="45px" height="55px">
                                                                </a>
                                                                <a id="quick-view-thumbnail-2" href="#" class="simpleLens-thumbnail-wrapper"
                                                                   data-lens-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/large/polo-shirt-3.png"
                                                                   data-big-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/medium/polo-shirt-3.png">
                                                                    <img src="~/MarkUps-dailyShop/dailyShop/img/view-slider/thumbnail/polo-shirt-3.png" width="45px" height="55px">
                                                                </a>

                                                                <a id="quick-view-thumbnail-3" href="#" class="simpleLens-thumbnail-wrapper"
                                                                   data-lens-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/large/polo-shirt-4.png"
                                                                   data-big-image="../../MarkUps-dailyShop/dailyShop/img/view-slider/medium/polo-shirt-4.png">
                                                                    <img src="~/MarkUps-dailyShop/dailyShop/img/view-slider/thumbnail/polo-shirt-4.png" width="45px" height="55px">
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- Modal view content -->
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <div class="aa-product-view-content">
                                                        <h3>T-Shirt</h3>
                                                        <div class="aa-price-block">
                                                            <span class="aa-product-view-price">$34.99</span>
                                                            <p class="aa-product-avilability">Avilability: <span>In stock</span></p>
                                                        </div>
                                                        <p id="quick-view-description">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Officiis animi, veritatis quae repudiandae quod nulla porro quidem, itaque quis quaerat!</p>
                                                        <div class="aa-prod-quantity">
                                                            <form action="">
                                                                <select id="quantityQuickView" name="select-quantity">
                                                                    <option value="1" selected>1</option>
                                                                    <option value="2">2</option>
                                                                    <option value="3">3</option>
                                                                    <option value="4">4</option>
                                                                    <option value="5">5</option>
                                                                </select>
                                                            </form>
                                                            <p class="aa-prod-category">
                                                                Category: <a href="#">Polo T-Shirt</a>
                                                            </p>
                                                        </div>
                                                        <div class="aa-prod-view-bottom">
                                                            <a href="#" id="add-cart" class="aa-add-to-cart-btn addCartInQuickView"><span class="fa fa-shopping-cart"></span>Add To Cart</a>
                                                            <a href="#" id="view-detail" class="aa-add-to-cart-btn">View Details</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div><!-- /.modal-content -->
                                </div><!-- /.modal-dialog -->
                            </div>
                            <!-- / quick view modal -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Products section -->
<!-- banner section -->
